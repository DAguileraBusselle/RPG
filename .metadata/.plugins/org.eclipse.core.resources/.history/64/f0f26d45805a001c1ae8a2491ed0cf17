package entidades;

public class Monstruo {

	private String clase;
	private int nivel;
	private int vida;
	private int danio;

	public Monstruo(String clase, int nivel, int vida, int danio) {
		this.clase = clase;
		this.nivel = nivel;
		this.vida = vida;
		this.danio = danio;
	}
	
	//public monstruo generarMonstro() {
		
