package entidades;

public class ObjetoPasivo extends Objeto{

	public static String[] FUNCIONES = new String[] {"vida", "velocidad", "danio"};		
	
	private String funcion;
	private String parametroFuncion;
	
	
	public ObjetoPasivo(String nombre, String descripcion, String funcion, String parametroFuncion) {
		super(nombre, descripcion);
		this.funcion = funcion;
		this.parametroFuncion = parametroFuncion;
	}




	public String getFuncion() {
		return funcion;
	}


	public void setFuncion(String funcion) {
		this.funcion = funcion;
	}




	public String getParametroFuncion() {
		return parametroFuncion;
	}




	public void setParametroFuncion(String parametroFuncion) {
		this.parametroFuncion = parametroFuncion;
	}
	
	
}
